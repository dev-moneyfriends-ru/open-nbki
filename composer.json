{
  "name": "mf-team/yii2-mf-nbch",
  "description": "Обмен данными с НБКИ",
  "type": "yii2-extension",
  "version": "dev-main",
  "keywords": [
    "yii2",
    "module",
    "nbch"
  ],
  "authors": [
    {
      "name": "Mikhail Glazkov",
      "email": "miha.o.g@ya.ru"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "~2.0.13",
    "yiisoft/yii2-httpclient": "~2.0.0",
    "ext-simplexml": "*",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-zip": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "goetas-webservices/xsd2php":"^0.3"
  },
  "autoload": {
    "psr-4": {
      "mfteam\\nbch\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
